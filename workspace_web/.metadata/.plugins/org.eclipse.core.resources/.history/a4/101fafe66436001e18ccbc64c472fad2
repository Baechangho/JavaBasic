<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript" src="https://code.jquery.com/jquery-3.7.0.min.js"></script>
<script type="text/javascript">
$(function() {
	
	//p태그들 선택
	var $p = $("p")
	
	console.log($p)
	
	//----------------------------------------------
	
	console.log('--$p.eq(idx)--')
	console.log($p.eq(0))	//jQuery DOM
	
	console.log('--$p.get(idx)--')
	console.log($p.get(1))	//JS DOM

	console.log('--$p[idx]--')
	console.log($p[2])	//JS DOM
	
	//----------------------------------------------
	
	//글자색 빨강으로 설정
	$p.get(1).style.color = "red"
	
	//글자색 파랑으로 설정
	$p.eq(2).css('color', 'blue')
	
	//----------------------------------------------

	//jQuery 객체의 .length 프로퍼티 이용하기
	for(var i = 0; i < $p.length; i++){
		$p.eq(i).
	}
	
	
	
	
})
</script>
</head>
<body>

<div>
	<span>하나</span>
	<p>둘</p>
	<pre>셋</pre>
	<p>넷</p>
	<p>다섯</p>
</div>

</body>
