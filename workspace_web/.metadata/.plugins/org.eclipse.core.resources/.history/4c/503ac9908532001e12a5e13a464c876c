<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">

//NaN, Not a Number

console.log(NaN)
console.log(NaN, typeof NaN)

console.log(NaN + 12345)

console.log("------------------------")

//parseInt() 내장함수
//매개변수의 문자를 숫자로 변환(치환)하는 함수
//숫자 형식이 아니면 NaN을 반환한다

console.log(parseInt('3456')) //'3456' -> 3456

console.log(parseInt('589QWER'))	//'589QWER' -> 589
console.log(parseInt('ABC8745'))	//'ABC8745' -> NaN

console.log("------------------------")

//parseFloat()

console.log(parseFloat("345.376"))
console.log(parseInt("345.376"))

console.log(parseFloat("7687"))

console.log("------------------------")

//Number()
console.log(Number("5874.37"))
console.log(Number("778"))

console.log("------------------------")

//Number 타입
var n1 = 5632.2345;

//숫자.toString(n)
//숫자 데이터를 문자데이터로 변환한다
//n진법으로 변환하며, 생략하면 10진법

console.log(n1.toString()) //"5632.2345"	(10진법)
console.log(n1.toString(2)) //"5632.2345"	(2진법)
console.log(n1.toString(8)) //"5632.2345"	(8진법)
console.log(n1.toString(16)) //"5632.2345"	(16진법)


//숫자.toFixed(n)
//변환된 문자열의 소수점 자리수를 지정할 수 있다
//n을 생략하면 소수점 이하 생략
console.log(n1.toFixed())
console.log(n1.toFixed(3))

</script>
</head>
<body>

</body>
</html>