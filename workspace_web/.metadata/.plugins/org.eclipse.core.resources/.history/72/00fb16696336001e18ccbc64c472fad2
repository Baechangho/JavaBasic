<script type="text/javascript">
$(function() {
	
	//p태그들 선택
	var $p = $("p")
	
	console.log($p)
	
	//----------------------------------------------
	
	console.log('--$p.eq(idx)--')
	console.log($p.eq(0))	//jQuery DOM
	
	console.log('--$p.get(idx)--')
	console.log($p.get(1))	//JS DOM

	console.log('--$p[idx]--')
	console.log($p.get(1))	//JS DOM

})
</script>
